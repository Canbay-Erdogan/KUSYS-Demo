@model KusysDemo.Entities.Student;
@{
    ViewData["Title"] = "Student Index";
    Layout = "_Layout";
}

<table class="table">
    <thead>
        <tr>
            <th>
                FirstName
            </th>
            <th>
                LastName
            </th>
            <th>
                BirthDate
            </th>
            <th>
                Actions
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>
                @Html.DisplayFor(modelItem => Model.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model.BirthDate)
            </td>
        </tr>
    </tbody>
</table>